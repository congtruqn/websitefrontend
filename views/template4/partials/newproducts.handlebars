<div class="newproducttitle">
	<p>Sản phẩm mới</p>
</div>
<div class="newproductcont">
  <div class="row">
    {{#each newproducts as |listnews|}}
                  <div class="col-xs-12">
                    <div class="new_product_item">
                        <div class="new_product_images">
                          <a href="/{{listnews.seo_url}}"><img src="/static/{{listnews.image_path}}thumb/{{listnews.image}}" alt={{listnews.alt}}></a>
                        </div>
                        <div class="new_product_detail">
                          <p class="new_product_name"><a href="/{{listnews.seo_url}}">{{{listnews.productname}}}</a></p>
                          <div id="clear"></div>
                            <div class="product_price"><p>{{listnews.pricebeauty}}₫</p></div>
                            <div class="sale_pricebeauty"><p>{{listnews.sale_pricebeauty}}₫</p></div>
                          <div id="clear"></div>
                        </div>
                      </div>
                  </div> 
    {{/each}}
  </div>
  <div id="clear"></div>
</div>
