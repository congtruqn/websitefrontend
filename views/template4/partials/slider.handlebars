<link rel="stylesheet" type="text/css" href="/layout/{{siteinfo.template}}/tp-banner/style.css" media="screen" />
<link rel="stylesheet" type="text/css" href="/layout/{{siteinfo.template}}/tp-banner/navstylechange.css" media="screen" />
<link rel="stylesheet" type="text/css" href="/layout/{{siteinfo.template}}/tp-banner/settings.css" media="screen" />
<script type="text/javascript" src="/layout/{{siteinfo.template}}/tp-banner/jquery.themepunch.plugins.min.js"></script>
<script type="text/javascript" src="/layout/{{siteinfo.template}}/tp-banner/jquery.themepunch.revolution.min.js"></script>
<div class="tp-banner-container">
  <div class="tp-banner" >
      <ul>
          {{#each banners as |banner|}}
            <li data-transition="fade" data-slotamount="7" data-masterspeed="500">
              <img src="/static/{{banner.image_path}}"   alt="slidebg1"  data-bgfit="cover" data-bgposition="center top" data-bgrepeat="no-repeat">
            </li>
            {{/each}}
      </ul>
      <div class="tp-bannertimer"></div>
  </div>
</div>


      <!-- END REVOLUTION SLIDER -->
<script type="text/javascript">
  var revapi;
  jQuery(document).ready(function() {
  revapi = jQuery('.tp-banner').revolution(
    {
      delay:5000,
      startwidth:1050,
      startheight:270,
      hideThumbs:10,
      fullWidth:"off",
    });
  }); //ready
</script>