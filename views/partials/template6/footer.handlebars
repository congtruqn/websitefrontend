<div class="footer-container">
  <div class="container">
    <div class="row">
      {{#each sitefooter as |footer|}}
        <div class="col-md-2 col-xs-6">
          <span class="fmn">{{footer.footer_name}}</span>
          {{#each footer.footer_child as |childfooter|}}
            <p><a href="/{{childfooter.child_url}}">{{{childfooter.child_name}}}</a></p>
          {{/each}}
        </div>
      {{/each}}
      <div class="col-md-5 col-sm-7 col-xs-12">
          <span class="fmn">Liên hệ với gianuoctannha.com</span>
            <div class="footer_address">
                  {{siteinfo.company_name}}
            </div>
            <div class="footer_address">
              <i class="fa fa-map-marker" aria-hidden="true"></i>
                  Điện thoại: {{siteinfo.phone}}
            </div>
            <div class="footer_address">
                  <i class="fa fa-phone" aria-hidden="true"></i>
                  Điện thoại: {{siteinfo.hotline}}
            </div>
            <div class="footer_address">
                  <i class="fa fa-envelope" aria-hidden="true"></i>
                  Email: {{siteinfo.email}}
            </div>

          <span class="fmn">Kết nối với giayfutsal.com.vn</span>



      </div>
      <div id="clear"></div>
  </div>
</div>

<div class="footer-bottom">
</div>