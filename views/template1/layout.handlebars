				}
});
$(function(){
  var header = $("header"),
      yOffset = 0,
      triggerPoint = 120;
  $(window).scroll(function(){
    //console.log($(window).scrollTop())
    yOffset = $(window).scrollTop();
    if(yOffset >= triggerPoint){
      header.addClass("minimized");
    }else{
      header.removeClass("minimized");
    }
    
  });
});
</script>
