<script type="text/javascript">
	function DoSubmitSingleExts() {
		var txt = $('#ime').val().trim();
		window.location = '/tim-kiem?key=' + txt.toLowerCase();
	};
</script >
<script type="text/javascript">
        function sent() {
          var name = document.getElementById('name').value;
          var phone = document.getElementById('phone').value;
          var email = document.getElementById('email').value;
          var dataString = 'name='+name+'&email='+email+'&phone='+phone;
          //alert(dataString);
          if(email==""||phone==""||name==""){
              alert('Bạn vui lòng nhập đầy đủ thông tin');
          }
          else{
            $.ajax({
                    type: "POST",
                    url: '/registertouse?'+dataString,
                    data: { email: email },
                    success: function (result) {
                      document.getElementById("register_info").style.display = "none";
                      document.getElementById("thank_register").style.display = "block";
                         //alert(result);
                    }
                });
          }
          
        }
</script>

