<div class="header-left">
  <div id="logo">
    <a href="/">
      <img class="img-responsive" alt="Your Store" title="Giày đá bóng chính hãng"
        src="/static{{{siteinfo.image_path}}}">
    </a>
  </div>
</div>
<div class="right-block">
  <div class="top-search">
    <div class="field search">
      <div class="control">
        <input type="text" value="" placeholder="Vui lòng nhập từ khóa..." class="input-text" maxlength="128"
          role="combobox" aria-autocomplete="both" autocomplete="off" id="ime">
        <div class="actions" onclick="DoSubmitSingleExts();">
          <button type="submit" title="Search" class="action search"><span>Search</span></button>
        </div>
      </div>
    </div>
  </div>
</div>