<body>
  {{#if istemplate}}
  <div id="istemplate">
    <div class="istemplate_left">
      <img src="/images/logo2_small.png">
    </div>
    <div class="istemplate_right">
      <a href="http://tns.vn/templateregister?template={{siteinfo.customer_username}}" class="big-link"
        data-reveal-id="myModal" data-animation="fade">{{i18n "register_to_use"}}</a>
    </div>
    <div id="clear"></div>
  </div>
  {{/if}}
  <div class="wrapper">
    <div id="topmenu">
      <div class="topmenu_center">
        <div class="topmenu_left">
          <marquee behavior="scroll" direction="left">Hoàng Việt là nhà sản xuất trực tiếp các sản phẩm gỗ mỹ nghệ, hộp
            gỗ tại Việt Nam</marquee>
        </div>
        <div class="topmenu_right">
          <ul>
            <li class=""><a href="javascript:void(0);" onclick="setlang('en')"><img
                  src="http://gohoangviet.com/public/images/en.png" alt="English" title=" English "></a></li>
            <li><a href="javascript:void(0);" onclick="setlang('vi')"><img
                  src="http://gohoangviet.com/public/images/vi.png" alt="English" title=" English "></a></li>
          </ul>
        </div>
      </div>
    </div>
    <header>
      <div class="cont-header">
        <div class="container">
          {{>head}}
        </div>
        <div id="navition">
          <div class="container">
            <div class="center">
              <a href="#menu" class="mobimenuicon"><span></span></a>
              <div class="desktop_menu">
                <div class="desktop_menu_right">
                  {{>mainmenu}}
                </div>
                <div id="clear"></div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </header>

    <div class="content" id="content">
      <div class="container">
        {{{body}}}
      </div>
    </div>
  </div> <!-- /container -->
  {{>mobilenavi}}
  <footer class="footer">
    {{>footer}}
  </footer>
</body>
<script type="text/javascript">
  $('.pro_detail').delegate('.list_size', 'click', function () {
    $('.list_size').removeClass('sizeactive');
    $(this).addClass('sizeactive');
  });
</script>
<script type="text/javascript">
  function DoSubmitSingleExts() {
    var txt = $('#ime').val().trim();
    window.location = '/tim-kiem?key=' + txt.toLowerCase();
  };
</script>
<script>
  new Mmenu(document.querySelector('#menu'));
  document.addEventListener('click', (evnt) => {
    let anchor = evnt.target.closest('a[href^="#/"]');
    if (anchor) {
      alert('Thank you for clicking, but that\'s a demo link.');
      evnt.preventDefault();
    }
  });
</script>
<script type="text/javascript">
  $.ajax({
    dataType: "json",
    url: '/getcart',
    data: '',
    success: function (result) {
      document.getElementById('cart-total').innerHTML = result;
    }
  });
</script>
<script type="text/javascript">
  function sent() {
    var name = document.getElementById('name').value;
    var phone = document.getElementById('phone').value;
    var email = document.getElementById('email').value;
    var dataString = 'name=' + name + '&email=' + email + '&phone=' + phone;
    //alert(dataString);
    if (email == "" || phone == "" || name == "") {
      alert('Bạn vui lòng nhập đầy đủ thông tin');
    }
    else {
      $.ajax({
        type: "POST",
        url: '/registertouse?' + dataString,
        data: { email: email },
        success: function (result) {
          document.getElementById("register_info").style.display = "none";
          document.getElementById("thank_register").style.display = "block";
          //alert(result);
        }
      });
    }
  }
</script>
</html>